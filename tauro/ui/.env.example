# Tauro UI Environment Variables
# Copy this file to .env.local and customize as needed

# API Base URL (defaults to /api/v1 with Vite proxy)
# VITE_API_BASE_URL=/api/v1

# For production with different API host:
# VITE_API_BASE_URL=https://api.tauro.example.com/api/v1

# Enable debug mode
# VITE_DEBUG=true

# Default project ID (if not using project selector)
# VITE_DEFAULT_PROJECT_ID=default

# Polling intervals (milliseconds)
# VITE_RUNS_POLL_INTERVAL=5000
# VITE_RUN_DETAIL_POLL_INTERVAL=3000

# Max logs to keep in memory
# VITE_MAX_LOGS=1000
